{
  "functionName": "update",
  "description": "Updates all entities in the database, filtering by the properties given",
  "author": "mapikit",
  "version": "0.0.1",
  "inputParameters": {
    "query": { "type": "cloudedObject", "required": true },
    "newValues": { "type": "cloudedObject", "required": true }
  },
  "outputData": {
    "errorMessage": { "type": "$errorMessage", "required": false },
    "updatedCount": { "type": "number", "required": false }
  },
  "entrypoint": "index.ts",
  "mainFunction": "main",
  "customTypes": [
    {
      "name": "errorMessage",
      "type": {
        "message": { "type": "string" },
        "errorCode": { "type": "string" }
      }
    }
  ]
}