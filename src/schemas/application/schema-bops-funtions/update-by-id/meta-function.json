{
  "functionName": "updateById",
  "description": "Updates the values of properties of an entity based on a provided ID.",
  "author": "mapikit",
  "version": "0.0.1",
  "inputParameters": {
    "id": { "type": "string", "required": true },
    "valuesToUpdate": { "type": "cloudedObject", "required": true }
  },
  "outputData": {
    "errorMessage": { "type": "$errorMessage", "required": false },
    "updatedEntity": { "type": "cloudedObject", "required": false }
  },
  "entrypoint": "index.ts",
  "mainFunction": "main",
  "customTypes": [
    {
      "name": "errorMessage",
      "type": {
        "message": { "type": "string" },
        "errorCode": { "type": "string" }
      }
    }
  ]
}